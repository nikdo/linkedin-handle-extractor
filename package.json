{
  "name": "linkedin-handle-extractor",
  "version": "1.0.0",
  "description": "Extract and normalize LinkedIn profile paths from messy user input",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsc && concurrently \"tsc --watch\" \"live-server --watch=dist\""
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "live-server": "^1.2.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "linkedin",
    "profile",
    "path",
    "handle",
    "parser",
    "url"
  ],
  "license": "MIT"
}
